{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "routes": [
        {"handle": "filesystem"},
        {"src": "^/([0-9]{4})/([0-9]{2})/([0-9]{2})/(.*)/(.*\\..*)$", "dest": "/blog/$1/$2/$3/$4/$5"},
        {"src": "^/([0-9]{4})/([0-9]{2})/([0-9]{2})/(.*)/$", "dest": "/blog/$1/$2/$3/$4/$4.html"},
        {"src": "/(.*)", "dest": "/?view=home"}
    ]
}